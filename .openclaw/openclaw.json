{
  "agents": {
    "defaults": {
      "model": {
        "primary": "poe/Gemini-3-Flash"
      },
      "workspace": "/root/.openclaw/workspace"
    }
  },
  "plugins": {
    "slots": {
      "memory": "memory-core"
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "poe": {
        "baseUrl": "https://api.poe.com/v1",
        "apiKey": "${POE_API_KEY}",
        "api": "openai-completions",
        "models": [
          { "id": "Gemini-3-Flash", "name": "Gemini 3 Flash" },
          { "id": "Claude-Opus-4.6", "name": "Claude Opus 4.6" },
          { "id": "kimi-k2.5", "name": "Kimi K2.5" },
          { "id": "surpiphany", "name": "Surpiphany" },
          { "id": "claude-3.5-sonnet", "name": "Claude 3.5 Sonnet" },
          { "id": "claude-3-opus", "name": "Claude 3 Opus" },
          { "id": "gpt-4o", "name": "GPT-4o" },
          { "id": "gemini-flash", "name": "Gemini 2.0 Flash" },
          { "id": "orchestr8", "name": "Orchestr8" },
          { "id": "epiphanyai", "name": "EpiphanyAI" },
          { "id": "brutallyhonestai", "name": "BrutallyHonestAI" }
        ]
      }
    }
  },
  "channels": {
    "whatsapp": {
      "allowFrom": ["*"]
    }
  },
  "hooks": {
    "enabled": true,
    "token": "${HOOKS_TOKEN}"
  },
  "gateway": {
    "mode": "local",
    "trustedProxies": ["10.0.0.0/8", "100.64.0.0/10", "172.16.0.0/12"],
    "controlUi": {
      "allowInsecureAuth": true
    }
  }
}
